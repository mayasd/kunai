{
    "service": {
        "tags": ["master"],
        "port": 80,
        "check": {
            "script": "/root/kunai/libexec/check_redis.py",
            "interval": "10s",
	    "handlers": ["default"]
        },
	"raft": true
    },
    "generator": {
        "apply_on": "haproxy",
	"path": "/tmp/haproxy.cfg",
	"template": "haproxy.cfg",
	"command": "/etc/init.d/haproxy reload"
    }
}
